{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {    
    "start": "webpack-dev-server --history-api-fallback --config=config/webpack.dev.js",
    "build:dev": "webpack --config=config/webpack.dev.js",
    "build:prod": "SET NODE_ENV=production & webpack --config=config/webpack.prod.js",
    "build:server": "SET NODE_ENV=production & webpack --config=config/webpack.server.js --watch",
    "server:dev": "nodemon --inspect --watch config --watch src/server src/server/main.js",
    "server:prod": "SET NODE_ENV=production & node src/server/main.js",
    "dev": "nodemon --inspect --watch dist --watch config dist/server-bundle.js",
    "prod": "SET NODE_ENV=production & node dist/server-bundle.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "clean": "rimraf build/"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-minify": "^0.3.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-preset-stage-1": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-dom": "^1.7.0",
    "chai-jquery": "^2.0.0",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.10",
    "eslint-config-rallycoding": "^3.2.0",
    "express": "^4.16.3",
    "extract-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fetch-mock": "^6.0.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.1.0",
    "image-webpack-loader": "^4.2.0",
